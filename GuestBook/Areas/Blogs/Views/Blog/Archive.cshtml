@using DataAccessObjects
@model List<DataAccessObjects.Blog>
@{
    ViewBag.Title = "Archive";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>
    Archive</h2>
@{
    var blogList = Model as List<Blog>;
    var years = blogList.GroupBy(x => x.Year);
}
@*Blog list for a particular year/month*@
<ul>
    @foreach (var item in years)
    {
        <li>@item</li>
    }
</ul>
